{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Vagrant plugin manifest",
	"type": "object",
	"properties": {
        "plugins": {
            "type": "object",
            "properties": {
		        "vagrant": {
			        "type": "object",
